{
  "name": "react-js-rollup-library",
  "version": "1.0.0",
  "description": "A React JS starter code",
  "type": "module",
  "private": true,
  "author": "Arpit Malik",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "lerna run start --stream",
    "rm-dist": "rimraf dist --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "clean": "npm run rm-package-lock && npm run rm-node_modules",
    "build": "lerna run build --stream --no-bail",
    "lint-js": "eslint \"**/*.{mjs,cjs,js,jsx,mdx}\" --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint \"**/*.{css,scss}\"",
    "prettier": "prettier",
    "lint": "npm run lint-js && npm run lint-css && npm run prettier -- --check .",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-css:fix": "npm run lint-css -- --fix",
    "prettier:fix": "prettier --write --ignore-unknown",
    "lint:fix": "npm run lint-js:fix && npm run lint-css:fix && npm run prettier:fix -- .",
    "lint-staged": "lint-staged",
    "grant-permissions:husky": "chmod +x .husky/*",
    "prepare": "husky install && npm run grant-permissions:husky",
    "commit": "git-cz",
    "test": "lerna run test --stream",
    "test:coverage": "lerna run test:coverage --stream",
    "test:coverage:pre-commit": "lerna run test:coverage:pre-commit --stream",
    "knip": "knip",
    "knip:debug": "knip --debug",
    "knip:check-files": "knip --include files",
    "knip:w/o-devDependencies": "knip --exclude devDependencies",
    "knip-prod": "knip --production",
    "knip-prod:check-files": "knip --production --include files",
    "knip:pre-commit": "npm run knip-prod:check-files",
    "storybook": "lerna run storybook --stream",
    "storybook:build": "lerna run storybook:build --stream"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.4.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.5",
    "knip": "^5.27.4",
    "lerna": "^8.1.8",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-hudochenkov": "^8.0.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "stylelint-order": "^6.0.4",
    "validate-branch-name": "^1.3.1"
  }
}
