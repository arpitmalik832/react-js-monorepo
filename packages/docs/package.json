{
  "name": "docs",
  "version": "0.2.1",
  "description": "> TODO: description",
  "private": true,
  "author": "Arpit Malik",
  "license": "MIT",
  "engines": {
    "node": ">=20.17.0",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install dependencies')\"",
    "rm-build": "rimraf build --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "rm-yarn-lock": "rimraf yarn.lock",
    "clean": "yarn rm-yarn-lock && yarn rm-node_modules",
    "docusaurus": "docusaurus",
    "start": "docusaurus start --config docusaurus.config.staging.cjs",
    "build-w-rm": "docusaurus build --config docusaurus.config.staging.cjs",
    "build": "yarn rm-build && yarn build-w-rm",
    "start:beta": "docusaurus start --config docusaurus.config.beta.cjs",
    "build-w-rm:beta": "docusaurus build --config docusaurus.config.beta.cjs",
    "build:beta": "yarn rm-build && yarn build-w-rm:beta",
    "start:prod": "docusaurus start --config docusaurus.config.prod.cjs",
    "build-w-rm:prod": "docusaurus build --config docusaurus.config.prod.cjs",
    "build:prod": "yarn rm-build && yarn build-w-rm:prod",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "create-version": "yarn docusaurus docs:version",
    "lint-js": "eslint \"**/*.{mjs,cjs,js,jsx,mdx}\" --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint \"**/*.{css,scss}\"",
    "prettier": "prettier",
    "lint": "yarn lint-js && yarn lint-css && yarn prettier --check .",
    "lint-js:fix": "yarn lint-js --fix",
    "lint-css:fix": "yarn lint-css --fix",
    "prettier:fix": "prettier --write --ignore-unknown",
    "lint:fix": "yarn lint-js:fix && yarn lint-css:fix && yarn prettier:fix .",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:coverage:pre-commit": "jest --coverage --silent",
    "test:watch:quick": "jest --watch --runInBand",
    "test:coverage:quick": "jest --coverage --maxWorkers=50%",
    "test:coverage:pre-commit:quick": "jest --coverage --maxWorkers=50% --silent",
    "test:clearCache": "jest --clearCache",
    "knip": "knip",
    "knip:debug": "knip --debug",
    "knip:check-files": "knip --include files",
    "knip:w/o-devDependencies": "knip --exclude devDependencies",
    "knip-prod": "knip --production",
    "knip-prod:check-files": "knip --production --include files",
    "knip:pre-commit": "yarn knip-prod:check-files",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.27.4",
    "lint-staged": "^15.2.9",
    "rimraf": "^6.0.1",
    "stylelint": "^16.9.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4"
  },
  "dependencies": {
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@docusaurus/theme-classic": "^3.5.2",
    "@mdx-js/react": "^3.0.1",
    "@svgr/webpack": "^8.1.0",
    "clsx": "^2.1.1",
    "docusaurus-theme-search-typesense": "^0.22.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "url-loader": "^4.1.1"
  }
}
