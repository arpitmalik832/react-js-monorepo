{
  "name": "@arpitmalik832/react-js-rollup-monorepo-library",
  "version": "0.2.0",
  "description": "> TODO: description",
  "type": "module",
  "author": "Arpit Malik",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arpitmalik832/react-js-rollup-library-monorepo-starter"
  },
  "engines": {
    "node": ">=20.17.0",
    "yarn": ">=1.22.22"
  },
  "files": [
    "dist"
  ],
  "module": "dist/components/index.js",
  "exports": {
    "./icons/*": "./dist/assets/icons/*",
    ".": "./dist/components/index.js",
    "./index.css": "./dist/index.css",
    "./enums": "./dist/enums/index.js",
    "./hooks": "./dist/hooks/index.js",
    "./providers": "./dist/providers/index.js",
    "./redux": "./dist/redux/index.js",
    "./services": "./dist/services/index.js",
    "./styles/*": "./dist/styles/*",
    "./utils": "./dist/utils/index.js"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install dependencies')\"",
    "rm-dist": "rimraf dist --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "rm-yarn-lock": "rimraf yarn.lock",
    "clean": "yarn rm-yarn-lock && yarn rm-node_modules",
    "generate-styles": "style-dictionary build --config style-dictionary.config.js",
    "build-sass": "sass --no-source-map ./src/styles/index.scss ./static/styles/sass-generated/index.css && postcss ./static/styles/sass-generated/index.css -o ./static/styles/postcss-processed/index.css",
    "watch-sass": "sass --no-source-map --watch ./src/styles/index.scss ./static/styles/sass-generated/index.css && postcss ./static/styles/sass-generated/index.css -o ./static/styles/postcss-processed/index.css --watch",
    "generate-icons-list": "node scripts/generate_icons_list.js",
    "colors-css-to-mdx": "node scripts/colors_css_to_mdx.js",
    "colors-scss-to-mdx": "node scripts/colors_scss_to_mdx.js",
    "typography-scss-to-mdx": "node scripts/typography_scss_to_mdx.js",
    "build-w-rm": "yarn generate-styles && yarn generate-icons-list && yarn build-sass && cross-env LIB_ENV=staging rollup -c rollup.config.js && node build_utils/scripts/updateImportForMixins.js",
    "build": "yarn rm-dist && yarn build-w-rm",
    "build-w-rm:beta": "yarn generate-styles && yarn generate-icons-list && yarn build-sass && cross-env LIB_ENV=beta rollup -c rollup.config.js && node build_utils/scripts/updateImportForMixins.js",
    "build:beta": "yarn rm-dist && yarn build-w-rm:beta",
    "build-w-rm:prod": "yarn generate-styles && yarn generate-icons-list && yarn build-sass && cross-env LIB_ENV=production rollup -c rollup.config.js && node build_utils/scripts/updateImportForMixins.js",
    "build:prod": "yarn rm-dist && yarn build-w-rm:prod",
    "lint-js": "eslint \"**/*.{mjs,cjs,js,jsx,mdx}\" --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint \"**/*.{css,scss}\"",
    "prettier": "prettier",
    "lint": "yarn lint-js && yarn lint-css && yarn prettier --check .",
    "lint-js:fix": "yarn lint-js --fix",
    "lint-css:fix": "yarn lint-css --fix",
    "prettier:fix": "prettier --write --ignore-unknown",
    "lint:fix": "yarn lint-js:fix && yarn lint-css:fix && yarn prettier:fix .",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:coverage:pre-commit": "jest --coverage --silent",
    "test:watch:quick": "jest --watch --runInBand",
    "test:coverage:quick": "jest --coverage --maxWorkers=50%",
    "test:coverage:pre-commit:quick": "jest --coverage --maxWorkers=50% --silent",
    "test:clearCache": "jest --clearCache",
    "knip": "knip",
    "knip:debug": "knip --debug",
    "knip:check-files": "knip --include files",
    "knip:w/o-devDependencies": "knip --exclude devDependencies",
    "knip-prod": "knip --production",
    "knip-prod:check-files": "knip --production --include files",
    "knip:pre-commit": "yarn knip-prod:check-files",
    "lint-staged": "lint-staged",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "node scripts/serve_storybook.js"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-a11y": "^8.2.8",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-storysource": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/react-webpack5": "^8.2.8",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "express": "^4.19.2",
    "glob": "^11.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.27.4",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.42",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "storybook": "^8.2.8",
    "style-dictionary": "^4.1.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.9.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "web-vitals": "^4.2.3"
  }
}
