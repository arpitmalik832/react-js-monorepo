{
  "name": "react-js-rollup-library-docs",
  "version": "0.3.10-alpha.2",
  "description": "> TODO: description",
  "private": true,
  "author": "Arpit Malik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arpitmalik832/react-js-monorepo"
  },
  "engines": {
    "node": ">=20.17.0",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('pnpm') === -1) throw new Error('You must use pnpm to install dependencies')\"",
    "rm-build": "rimraf build --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "rm-yarn-lock": "rimraf yarn.lock",
    "clean": "pnpm rm-yarn-lock && pnpm rm-node_modules",
    "docusaurus": "docusaurus",
    "start": "docusaurus start --config docusaurus.config.staging.cjs",
    "start:beta": "docusaurus start --config docusaurus.config.beta.cjs",
    "start:prod": "docusaurus start --config docusaurus.config.prod.cjs",
    "build": "pnpm rm-build && docusaurus build --config docusaurus.config.staging.cjs",
    "build:beta": "pnpm rm-build && docusaurus build --config docusaurus.config.beta.cjs",
    "build:prod": "pnpm rm-build && docusaurus build --config docusaurus.config.prod.cjs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --config docusaurus.config.staging.cjs",
    "serve:beta": "docusaurus serve --config docusaurus.config.beta.cjs",
    "serve:prod": "docusaurus serve --config docusaurus.config.prod.cjs",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "create-version": "pnpm docusaurus docs:version",
    "lint-js": "eslint --quiet",
    "lint-js:debug": "eslint --debug",
    "lint-js:print-config": "eslint --print-config eslintConfigProcessed.js",
    "lint-css": "stylelint",
    "prettier": "prettier",
    "lint": "pnpm lint-js \"**/*.{mjs,cjs,js,jsx,mdx}\" && pnpm lint-css \"**/*.{css,scss}\" && pnpm prettier --check .",
    "lint-js:fix": "pnpm lint-js --fix",
    "lint-css:fix": "pnpm lint-css --fix strict",
    "prettier:fix": "prettier --write --ignore-unknown",
    "lint:fix": "pnpm lint-js:fix \"**/*.{mjs,cjs,js,jsx,mdx}\" && pnpm lint-css:fix \"**/*.{css,scss}\" && pnpm prettier:fix .",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:clearCache": "jest --clearCache",
    "test:watch:quick": "pnpm test:watch --maxWorkers=50%",
    "test:watch:inBand": "pnpm test:watch --runInBand",
    "test:coverage:silent": "pnpm test:coverage --silent",
    "test:coverage:quick": "pnpm test:coverage --maxWorkers=50%",
    "test:coverage:inBand": "pnpm test:coverage --runInBand",
    "test:coverage:silent:quick": "pnpm test:coverage:silent --maxWorkers=50%",
    "test:coverage:silent:inBand": "pnpm test:coverage:silent --runInBand",
    "knip": "knip",
    "knip:debug": "pnpm knip --debug",
    "knip:check-files": "pnpm knip --include files",
    "knip:w/o-devDependencies": "pnpm knip --exclude devDependencies",
    "knip:prod": "pnpm knip --production",
    "knip:prod:check-files": "pnpm knip:prod --include files"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-syntax-import-attributes": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "^9.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.43.6",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "stylelint": "^16.13.2",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-hudochenkov": "^11.0.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/theme-classic": "^3.7.0",
    "@mdx-js/react": "^3.1.0",
    "@svgr/webpack": "^8.1.0",
    "clsx": "^2.1.1",
    "docusaurus-theme-search-typesense": "^0.23.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "url-loader": "^4.1.1"
  }
}
